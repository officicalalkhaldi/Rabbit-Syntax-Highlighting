{
  "name": "rabbitsyntax",
  "displayName": "Rabbit Syntax Highlighting",
  "publisher": "RabbitTeam",
  "version": "0.1.0",
  "description": "Syntax highlighting for Rabbit language",
  "icon": "images/icon.png",
  "main": "extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/officicalalkhaldi/Rabbit-Syntax-Highlighting.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "activationEvents": [
    "onLanguage:rabbit"
  ],
  "contributes": {
    "languages": [
      {
        "id": "rabbit",
        "extensions": [".rabt"],
        "aliases": ["rabbit", "rabbit"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "rabbit",
        "scopeName": "source.rabt",
        "path": "./syntaxes/rabt.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "id": "rabbit-theme",
        "label": "Rabbit Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/rabt-theme.json"
      }
    ]
  },
  "files": [
    "extension.js",
    "syntaxes/rabt.tmLanguage.json",
    "themes/rabt-theme.json",
    "language-configuration.json"
  ],
  "devDependencies": {
    "typescript": "^4.5.5"
  },
  "license": "MIT"
}
